{
  "manifest_version": 2,
  "name": "CI Target Blank",
  "short_name": "CI_blank",
  "description": "Open GitHub CI icon links in another tab.",
  "version": "0.0.1",
  "author": "OSA Shunsuke",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery.min.js", "js/ci-target-blank.js" ],
      "run_at": "document_start"
    }
  ]
}
